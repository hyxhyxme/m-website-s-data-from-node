<div>
    {{each list}}
        <div class="items" data-id="{{$value.id}}">
            <div class="title line-ellipsis">
                <span>{{$value.nm}}</span>
                <span class="price-block">
                    <span class="price">{{$value.sellPrice}}</span><span class="q"> 元起</span>
                </span>
            </div>
            <div class="location-block box-flex">
                <div class="flex line-ellipsis">{{$value.addr}}</div>
                <div class="distance">{{$value.distance}}</div>
            </div>
            <div class="label-block">
                <div class="allowRefund" data-show="{{$value.tag.allowRefund}}">退</div>
                <div class="endorse" data-show="{{$value.tag.endorse}}">改签</div>
                <div class="snack" data-show="{{$value.tag.snack}}">小吃</div>
                <div class="vipTag" data-show="{{$value.tag.vipTag}}">折扣卡</div>
            </div>
            <div class="discount-block">
                <div>
                    <div class="discount-label normal card">
                        <img src="data:image/png;base64," alt="">     
                    </div>
                    <div class="discount-label-text">{{$value.promotion.cardPromotionTag}}</div>
                </div>
            </div>
        </div>
    {{/each}}
</div>